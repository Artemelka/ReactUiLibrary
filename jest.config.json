{
  "collectCoverage": true,
  "coverageReporters": [
    "lcov",
    "text"
  ],
  "transform": {
    "^.+\\.jsx?$": "babel-jest",
    "^.+\\.tsx?$": "ts-jest",
    "^.+\\.(css|less)$": "jest-css-modules"
  },
  "globals": {
    "NODE_ENV": "test"
  },
  "setupFiles": [
    "./setupTest.js"
  ],
  "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js|jsx)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json"
  ],
  "moduleDirectories": [
    "node_modules"
  ]
}
